{
  "name": "lighthouse-dashboard-server",
  "version": "4.1.0",
  "adonis-version": "4.1.0",
  "description": "Server side for a Lighthouse dashboard",
  "main": "server.js",
  "scripts": {
    "dev": "adonis serve --dev",
    "start": "adonis serve",
    "test": "node ace test",
    "fetch": "node ./scripts/reporting/fetch-report.js",
    "analyze": "node ./scripts/reporting/analyze-report.js",
    "remove": "node ./scripts/reporting/delete-report.js",
    "deploy": "cd .. && git subtree push --prefix server heroku master"
  },
  "author": "Jeremy Bratcher",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^5.0.8",
    "@adonisjs/auth": "^3.0.7",
    "@adonisjs/bodyparser": "^2.0.5",
    "@adonisjs/cli": "^4.0.12",
    "@adonisjs/cors": "^1.0.7",
    "@adonisjs/fold": "^4.0.9",
    "@adonisjs/framework": "^5.0.9",
    "@adonisjs/ignitor": "^2.0.8",
    "@adonisjs/lucid": "^6.1.3",
    "@adonisjs/mail": "^3.0.10",
    "@adonisjs/persona": "^1.0.5",
    "@adonisjs/validator": "^5.0.6",
    "adonis-lucid-update-or-create": "^1.0.2",
    "lighthouse": "^5.6.0",
    "pg": "^8.0.2",
    "url-parse": "^1.4.7"
  },
  "autoload": {
    "App": "./app"
  }
}
